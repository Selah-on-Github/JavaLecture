package chap99_homework.hw231019.animal;

// 고양이과 클래스
public abstract class TheCatFamily extends Mammal {
	
	public int volume;
	
	public TheCatFamily() {
		volume = 1;
	}
	
//	public void eat () {
//		System.out.println("고양이과는 식사를 한다.");
//	}
//	
//	public void sleep() {
//		System.out.println("고양이과는 누워서 잔다.");
//	}
	
	public abstract void eat();
	public abstract void sleep();


	
	
	public void bigVoice() {
		this.volume++;
	}
	
	public void smallVoice() {
		this.volume--;
	}
	

}
